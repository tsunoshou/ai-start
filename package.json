    "@tanstack/react-query": "^5.28.2",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.6",
    "lucide-react": "^0.359.0",
    "next": "^14.1.3",
    "next-auth": "^5.0.0-beta.3",
    "next-intl": "^3.9.1",
    "postcss": "^8.4.35",
    "postgres": "^3.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.2.1",
    "tailwindcss": "^3.4.1",
    "tsyringe": "^4.8.0",
    "zod": "^3.22.4"
  },
