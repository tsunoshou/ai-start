{
  "name": "ai-start",
  "version": "0.3.1",
  "private": true,
  "engines": {
    "node": "22.x",
    "npm": ">=10.9.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit --skipLibCheck",
    "check": "npm run lint && npm run format:check && npm run type-check",
    "fix": "npm run lint:fix && npm run format",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:e2e": "playwright test --workers=1",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:codegen": "playwright codegen",
    "coverage": "vitest run --coverage",
    "prebuild-check": "npm run lint && npx eslint . && npm run type-check && npm run test:unit && npm run test:e2e",
    "full-check": "npm run prebuild-check && npm run build",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "db:test": "tsx infrastructure/database/utils/test-db-connection.ts",
    "db:migrate": "node-pg-migrate up -m infrastructure/database/migrations",
    "db:migrate:down": "node-pg-migrate down -m infrastructure/database/migrations",
    "prepare": "husky",
    "fmt": "prettier --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsunoshou/ai-start.git"
