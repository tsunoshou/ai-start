# ===============================================================
# AiStart 開発環境用環境変数設定サンプルファイル
# ===============================================================
# このファイルをコピーして.env.developmentを作成し、実際の値を設定してください
# ===============================================================

# ==== 基本設定 ====
NODE_ENV=development
APP_ENV=development
NEXT_PUBLIC_APP_URL=https://dev.ai-start.net

# ==== データベース接続設定 ====
# 現在の環境のデータベース接続
DATABASE_URL=postgresql://postgres:postgres@localhost:54322/postgres

# 環境別データベース設定（マイグレーションコマンドで使用）
DATABASE_URL_DEVELOPMENT=postgresql://postgres:postgres@localhost:54322/postgres
DATABASE_URL_STAGING=postgresql://postgres:password@db.example.supabase.co:5432/ai_start_staging
DATABASE_URL_PRODUCTION=postgresql://postgres:password@db.example.supabase.co:5432/ai_start_prod

# ==== Supabase設定 ====
# 現在の環境のSupabase設定
NEXT_PUBLIC_SUPABASE_URL=http://localhost:54321
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.example-anon-key
SUPABASE_SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.example-service-role-key

# 環境別Supabase設定
NEXT_PUBLIC_SUPABASE_URL_DEVELOPMENT=http://localhost:54321
NEXT_PUBLIC_SUPABASE_ANON_KEY_DEVELOPMENT=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.example-anon-key
SUPABASE_SERVICE_ROLE_KEY_DEVELOPMENT=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.example-service-role-key

NEXT_PUBLIC_SUPABASE_URL_STAGING=https://example-staging.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY_STAGING=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.example-anon-key
SUPABASE_SERVICE_ROLE_KEY_STAGING=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.example-service-role-key

NEXT_PUBLIC_SUPABASE_URL_PRODUCTION=https://example-prod.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY_PRODUCTION=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.example-anon-key
SUPABASE_SERVICE_ROLE_KEY_PRODUCTION=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.example-service-role-key

# ==== 認証設定 ====
AUTH_SECRET=development-auth-secret-example-key
AUTH_URL=https://dev.ai-start.net

# ==== 国際化設定 ====
DEFAULT_LOCALE=ja
SUPPORTED_LOCALES=ja,en

# ==== OAuth設定 ====
# 開発環境用OAuthキー
GOOGLE_CLIENT_ID=123456789012-example.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=GOCSPX-example_secret_key
GITHUB_CLIENT_ID=abcdef1234567890
GITHUB_CLIENT_SECRET=abcdef1234567890abcdef1234567890

# ==== AI設定 ====
# 開発環境ではAPIキーの使用量制限を設定することを推奨
OPENAI_API_KEY=sk-example1234567890abcdefghijklmnopqrstu
ANTHROPIC_API_KEY=sk-ant-example01234567-abcdefghijklmnopqrstuvwxyz
GEMINI_API_KEY=AI-example-key-1234567890

# ==== ストレージ設定 ====
BLOB_READ_WRITE_TOKEN=vercel-blob-rw-example-123456789-1234567890abcdef

# ==== ログ設定 ====
# 開発環境では詳細なログが役立つ
LOG_LEVEL=info 